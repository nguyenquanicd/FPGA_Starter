

/***************************** Include Files *******************************/
#include "PwmIp_Demo.h"
/************************** Function Definitions ***************************/
void PWMIP_DEMO_writePwmPeriod(u32 BaseAddress,u32 pwmPeriod){
	PWMIP_DEMO_mWriteReg(BaseAddress,PWMIP_DEMO_S00_AXI_SLV_REG0_OFFSET,pwmPeriod);
}

uint32_t PWMIP_DEMO_readPwmPeriod(u32 BaseAddress){
	return (PWMIP_DEMO_mReadReg(BaseAddress,PWMIP_DEMO_S00_AXI_SLV_REG0_OFFSET));
}
